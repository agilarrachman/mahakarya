{
  "version": 2,
  "builds": [
    {
      "src": "index.php",
      "use": "@vercel-community/php"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "continue": true
    },
    {
      "src": "/photo/(.*)",
      "dest": "/photo/$1",
      "continue": true
    },
    {
      "src": "/database/(.*)",
      "dest": "/database/$1",
      "continue": true
    },
    {
      "src": "/(.*\\.(css|js|jpg|jpeg|png|gif|svg|ico|woff|woff2|ttf|eot|pdf|zip))",
      "dest": "/$1",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ],
  "env": {
    "CI_ENV": "production"
  }
}
